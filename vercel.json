{
    "version": 2,
    "builds": [
      { "src": "/app.py", "use": "@vercel/python" }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/app.py"
      }
    ],
    "env": {
      "FLASK_ENV": "production",
      "FLASK_APP": "app.py",
      "DATABASE_URL": "users.db"
    }
  }
  